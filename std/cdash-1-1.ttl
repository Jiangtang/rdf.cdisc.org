  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#Domain.SC> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.16> ;
  cdiscs:recommendationText "The Subject Characteristics domain describes the essential characteristics of the study subjects."^^xsd:string ;
