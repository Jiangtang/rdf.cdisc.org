  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.DA.DADAT> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.8.1> ;
  cdiscs:recommendationText "The date study treatment dispensed / returned should be recorded for each dispensation for a study with multiple periods or multiple products dispensed."^^xsd:string ;
.
<http://rdf.cdisc.org/std/cdash-1-1#Recommendation.DA.5.8.1.12>
  rdf:type cdiscs:Recommendation ;
  mms:ordinal "23"^^xsd:positiveInteger ;
  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.DA.DADAT> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.8.1> ;
  cdiscs:recommendationText "For the SDTM-based dataset, the SDTM IG variable DADTC is derived by putting the CDASH Date study treatment dispensed or returned (DADAT) into DADTC using the ISO 8601 format."^^xsd:string ;
.
<http://rdf.cdisc.org/std/cdash-1-1#Recommendation.DA.5.8.1.13>
  rdf:type cdiscs:Recommendation ;
