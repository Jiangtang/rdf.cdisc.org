  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.DA.DADAT> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.8.1> ;
  cdiscs:recommendationText "For more detail see cdash:Section.3.0."^^xsd:string ;
.
<http://rdf.cdisc.org/std/cdash-1-1#Recommendation.DA.5.8.1.14>
  rdf:type cdiscs:Recommendation ;
  mms:ordinal "25"^^xsd:positiveInteger ;
