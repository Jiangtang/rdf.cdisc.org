  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.13> ;
  cdiscs:recommendationText "The sponsor may choose to capture a complete date or any variation thereof (e.g., month & year or year, etc.)."^^xsd:string ;
.
<http://rdf.cdisc.org/std/cdash-1-1#Recommendation.MH.5.13.0.27>
  rdf:type cdiscs:Recommendation ;
  mms:ordinal "27"^^xsd:positiveInteger ;
  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.MH.MHSTDAT> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.13> ;
  cdiscs:recommendationText "For the SDTM-based dataset, the SDTM IG variable MHSTDTC is derived from the CDASH Start Date (MHSTDAT) using the ISO 8601 format."^^xsd:string ;
.
