  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.8.1> ;
  cdiscs:recommendationText "This packaging identifier (e.g. kit number, bottle label, vial identifier) may be collected in different ways (e.g. affixing label onto CRF or scanning a bar code)."^^xsd:string ;
.
<http://rdf.cdisc.org/std/cdash-1-1#Recommendation.DA.5.8.1.16>
  rdf:type cdiscs:Recommendation ;
  mms:ordinal "27"^^xsd:positiveInteger ;
  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.DA.DAREFID> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.8.1> ;
  cdiscs:recommendationText "For some study dosing regimens, greater granularity for treatment identifiers may be needed. In this situation, sponsors may need to use additional variables."^^xsd:string ;
