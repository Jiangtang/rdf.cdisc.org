  cdiscs:about <http://rdf.cdisc.org/std/cdash-1-1#DataElement.MH.MHOCCUR> ;
  cdiscs:partOfSection <http://rdf.cdisc.org/std/cdash-1-1#Section.5.13> ;
  cdiscs:recommendationText "MHOCCUR should only be used if the condition pre-printed on the CRF elicits a \"Yes\" or \"No\" response.  MHOCCUR should not be used if the conditions are collected on the CRF in a manner that requires a free-flow text response."^^xsd:string ;
